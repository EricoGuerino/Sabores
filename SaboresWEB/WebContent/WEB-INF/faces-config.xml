<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<navigation-rule>
		<navigation-case>
			<from-outcome>toSabores</from-outcome>
			<to-view-id>/pages/templates/sabores.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/cadastroCategoria.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toAdministracao</from-outcome>
			<to-view-id>/pages/protected/admin/administracao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/cadastroFabricante.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toAdministracao</from-outcome>
			<to-view-id>/pages/protected/admin/administracao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/cadastroTipoEstabelecimento.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toAdministracao</from-outcome>
			<to-view-id>/pages/protected/admin/administracao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/cadastroProduto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toAdministracao</from-outcome>
			<to-view-id>/pages/protected/admin/administracao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/modificarPrecoEstoque.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toAdministracao</from-outcome>
			<to-view-id>/pages/protected/admin/administracao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/templates/sabores.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toSobre</from-outcome>
			<to-view-id>/pages/public/sobre.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toProdutos</from-outcome>
			<to-view-id>/pages/public/produtos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toContato</from-outcome>
			<to-view-id>/pages/public/contato.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toCadastro</from-outcome>
			<to-view-id>/pages/public/cadastroCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toAdministracao</from-outcome>
			<to-view-id>/pages/protected/admin/administracao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/public/sobre.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toSabores</from-outcome>
			<to-view-id>/pages/templates/sabores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/public/produtos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toSabores</from-outcome>
			<to-view-id>/pages/templates/sabores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/public/contato.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toSabores</from-outcome>
			<to-view-id>/pages/templates/sabores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/public/cadastroCliente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toSabores</from-outcome>
			<to-view-id>/pages/templates/sabores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/tabelasDeDados/listarCategorias.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>refresh</from-outcome>
			<to-view-id>/pages/protected/admin/tabelasDeDados/listarCategorias.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/tabelasDeDados/listarFabricantes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>refresh</from-outcome>
			<to-view-id>/pages/protected/admin/tabelasDeDados/listarFabricantes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/tabelasDeDados/listarTiposEstabelecimento.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>refresh</from-outcome>
			<to-view-id>/pages/protected/admin/tabelasDeDados/listarTiposEstabelecimento.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protected/admin/*</from-view-id>
		<navigation-case id="cadastroCategoria">
			<from-outcome>toCadastroCategoria</from-outcome>
			<to-view-id>/pages/protected/admin/cadastroCategoria.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toCadastroFabricante</from-outcome>
			<to-view-id>/pages/protected/admin/cadastroFabricante.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toCadastroTipoEstabelecimento</from-outcome>
			<to-view-id>/pages/protected/admin/cadastroTipoEstabelecimento.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toCadastroProduto</from-outcome>
			<to-view-id>/pages/protected/admin/cadastroProduto.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toCadastroPrecoEstoque</from-outcome>
			<to-view-id>/pages/protected/admin/modificarPrecoEstoque.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toCategorias</from-outcome>
			<to-view-id>/pages/protected/admin/tabelasDeDados/listarCategorias.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toFabricantes</from-outcome>
			<to-view-id>/pages/protected/admin/tabelasDeDados/listarFabricantes.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>toTiposEstabelecimento</from-outcome>
			<to-view-id>/pages/protected/admin/tabelasDeDados/listarTiposEstabelecimento.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>
</faces-config>
